<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">

<!--
	Theme that mimics IntelliJ IDEA's defaults.
	See theme.dtd and org.fife.ui.rsyntaxtextarea.Theme for more information.
-->
<RSyntaxTheme version="1.0">

   <!-- Omitting baseFont will use a system-appropriate monospaced. -->
   <!--<baseFont family="..." size="13"/>-->

   <!--  General editor colors. -->
   <background color="2b2b2b"/>                                                          <!-- done -->
   <caret color="bbbbbb"/>                                                               <!-- done -->
   <selection fg="ffffff" bg="526da5"/>                                                  <!-- not done -->
   <currentLineHighlight color="323232" fade="false"/>                                   <!-- done -->
   <marginLine fg="555555"/>                                                             <!-- not done -->
   <markAllHighlight color="ccccff"/>                                                    <!-- not done -->
   <markOccurrencesHighlight color="ccccff" border="false"/>                             <!-- not done -->
   <matchedBracket fg="3b514d" bg="3b514d" highlightBoth="true" animate="true"/>        <!-- done -->
   <hyperlinks fg="589cf6"/>                                                             <!-- not done -->
   <secondaryLanguages>
      <language index="1" bg="fff0cc"/>                                                  <!-- not done -->
      <language index="2" bg="dafeda"/>                                                  <!-- not done -->
      <language index="3" bg="ffe0f0"/>                                                  <!-- not done -->
   </secondaryLanguages>

   <!-- Gutter styling. -->
   <gutterBorder color="555555"/>                                                        <!-- done -->
   <lineNumbers fg="5e6164"/>                                                            <!-- done -->
   <foldIndicator fg="555555" iconBg="2b2b2b"/>                                          <!-- done -->
   <iconRowHeader activeLineRange="ff0000"/>                                             <!-- not done -->

   <!-- Syntax tokens. -->
