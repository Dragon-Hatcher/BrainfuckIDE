      <style token="COMMENT_KEYWORD" fg="629755" bold="true" underline="true" italic="true"/>  <!-- done -->
      <style token="COMMENT_MARKUP" fg="808080" bg="e2ffe2" italic="true"/>              <!-- not done -->
      <style token="DATA_TYPE" fg="cc7832" bold="true"/>                                 <!-- done -->
      <style token="FUNCTION" fg="df664b"/>                                              <!-- not done -->
      <style token="LITERAL_BOOLEAN" fg="cc7832" bold="true"/>                           <!-- done -->
